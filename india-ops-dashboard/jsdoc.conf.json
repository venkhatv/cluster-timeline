{ 
  "plugins": ["node_modules/jsdoc-babel"],
  "babel": {
      "extensions": ["js", "es6", "jsx"]
  },
  "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "include": ["./src"],
        "exclude": []
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
          "outputSourceFiles": true,
          "includeDate": true
        }
    },
    "opts": {
      "template": "templates/default",  
        "encoding": "utf8",               
        "destination": "./out/",          
        "recurse": true
  }
}